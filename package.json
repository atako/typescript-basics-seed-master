    "eslint-plugin-typescript": "^0.12.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.2",
    "ts-jest": "^23.1.4",
    "typescript": "3.0.3",
    "typescript-eslint-parser": "^18.0.0",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7"
  },
